  "query": "\n            SELECT\n                id,\n                i.name       AS \"item_name\",\n                quantity,\n                i.type_id,\n                i.category_id,\n                i.group_id,\n                cost,\n                source\n            FROM project_market pm\n            JOIN items i\n              ON i.type_id = pm.type_id\n            WHERE pm.project_id = $1\n            ORDER BY item_name\n        ",
