  "query": "\n                SELECT\n                    c.character_id,\n                    c.character_name,\n                    c.corporation_id,\n                    c.corporation_name,\n                    c.alliance_id,\n                    c.alliance_name\n                FROM jwt_refresh_token jrt\n                JOIN character c ON jrt.character_id = c.character_id\n                WHERE token_hash = $1\n                AND jrt.character_id = $2\n            ",
