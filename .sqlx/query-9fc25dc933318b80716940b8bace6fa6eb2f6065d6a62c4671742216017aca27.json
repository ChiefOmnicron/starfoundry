  "query": "\n            SELECT\n                pgm.character_id,\n                accepted,\n                permission,\n                (pg.owner = pgm.character_id) AS is_owner\n            FROM project_group_member pgm\n            JOIN project_group pg ON pg.id = pgm.project_group_id\n            WHERE project_group_id = $1\n        ",
