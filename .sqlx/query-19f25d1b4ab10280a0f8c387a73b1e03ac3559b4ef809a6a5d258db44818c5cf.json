  "query": "\n            SELECT\n                type_id,\n                volume,\n                meta_group_id,\n                repackaged,\n                i.category_id,\n                i.group_id,\n                i.name,\n                c.name AS \"category_name\",\n                g.name AS \"group_name\"\n            FROM item i\n            JOIN category c ON i.category_id = c.category_id\n            JOIN groups g ON i.group_id = g.group_id\n            WHERE type_id = $1\n        ",
