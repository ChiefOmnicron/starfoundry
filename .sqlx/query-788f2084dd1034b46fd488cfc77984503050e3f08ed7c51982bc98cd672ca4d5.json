{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                DISTINCT(pj.type_id) AS \"type_id: TypeId\",\n                depends_on,\n\n                i.name,\n                i.volume,\n                i.category_id,\n                i.group_id,\n                i.meta_group_id,\n\n                id,\n                runs,\n                status AS \"status: ProjectJobStatus\",\n                cost,\n                job_id,\n                structure_id\n            FROM project_job pj\n            JOIN blueprint_dependency bd ON pj.type_id = bd.ptype_id\n            JOIN item i ON i.type_id = pj.type_id\n            WHERE project_id = $1\n        ",
